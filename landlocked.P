:- include(mondial).
% Name of all countries that have no coast = landlocked country (ll)
a(N) :- country(N,_C,_,_,_,_), not geo_sea(_,_C,_).
% Name of all countries that are landlocked and have only landlocked neighbors
neighbor(C1, C2) :- borders(C1,C2,_), not geo_sea(_,C2,_).
b(N) :- country(N,_C,_,_,_,_), not geo_sea(_,_C,_), not neighbor(C,_).
